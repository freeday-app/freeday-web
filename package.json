{
    "name": "freeday-web",
    "version": "1.0.3",
    "author": "Coddity",
    "description": "Freeday web client",
    "dependencies": {
        "@babel/core": "^7.17.9",
        "@babel/eslint-parser": "^7.17.0",
        "@blueprintjs/core": "^3.54.0",
        "@blueprintjs/datetime": "^3.24.0",
        "dayjs": "^1.11.1",
        "ejs": "^3.1.7",
        "emoji-mart": "^3.0.1",
        "eslint": "^8.13.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-chai-friendly": "^0.7.2",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.4",
        "express": "^4.17.3",
        "query-string": "^7.1.1",
        "react": "^17.0.2",
        "react-csv": "^2.2.2",
        "react-dom": "^17.0.2",
        "react-icons": "^4.3.1",
        "react-image-file-resizer": "^0.4.7",
        "react-router-dom": "^5.3.1",
        "react-scripts": "^5.0.1",
        "react-select": "^5.3.0",
        "stylelint": "^14.7.1",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-order": "^5.0.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.21.1",
        "chai": "^4.3.6",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "mocha": "^9.2.2",
        "node-fetch": "^2.6.7",
        "playwright": "^1.21.1",
        "wait-on": "^6.0.1"
    },
    "scripts": {
        "start": "cross-env REACT_APP_ENV=dev PORT=8788 SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "start-no-browser": "cross-env REACT_APP_ENV=dev PORT=8788 BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "wait": "wait-on http-get://localhost:8788 --httpTimeout 180000 --timeout 180000",
        "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "eject": "react-scripts eject",
        "test": "concurrently --success first --kill-others \"npm run start-no-browser\" \"npm run wait && npm run e2e\"",
        "e2e": "npx playwright test --reporter list --config tests/e2e/conf/headless.js",
        "e2e-headed": "npx playwright test --reporter list --config tests/e2e/conf/headed.js",
        "e2e-headed-slow": "npx playwright test --reporter list --config tests/e2e/conf/headedSlow.js",
        "lint-js": "eslint .",
        "lint-js-fix": "eslint . --fix",
        "lint-css": "stylelint src/**/*.css",
        "lint-css-fix": "stylelint src/**/*.css --fix",
        "lint": "npm run lint-js && npm run lint-css",
        "lint-fix": "npm run lint-js-fix && npm run lint-css-fix"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
